<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.TableView?>
<GridPane alignment="CENTER" hgap="30" vgap="30" xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.tablesku.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <GridPane fx:id="buttonGrid" hgap="10" vgap="10" GridPane.rowIndex="0" GridPane.columnIndex="0">
        <Button fx:id="downloadFromTxt" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Button fx:id="downloadFromXml" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Button fx:id="uploadToTxt" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
        <Button fx:id="uploadToXml" GridPane.columnIndex="3" GridPane.rowIndex="0"/>
        <Button fx:id="importFromDb" GridPane.columnIndex="4" GridPane.rowIndex="0"/>
        <Button fx:id="exportToDb" GridPane.columnIndex="5" GridPane.rowIndex="0"/>
        <Button fx:id="loginToAllegro" GridPane.columnIndex="6" GridPane.rowIndex="0"/>
        <Button fx:id="downloadFromAllegro" GridPane.columnIndex="7" GridPane.rowIndex="0"/>

    </GridPane>

    <TableView fx:id="computerTable" GridPane.columnIndex="0" GridPane.rowIndex="1"></TableView>
    <Button fx:id="addNewRow" GridPane.columnIndex="0" GridPane.rowIndex="2"></Button>
</GridPane>
